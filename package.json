{
  "name": "@folio/platform-complete",
  "version": "1.0.1-SNAPSHOT",
  "license": "Apache-2.0",
  "scripts": {
    "build": "export NODE_OPTIONS=\"--max-old-space-size=4096 $NODE_OPTIONS\"; stripes build stripes.config.js",
    "stripes": "stripes",
    "start": "stripes serve stripes.config.js",
    "build-module-descriptors": "node ./build-module-descriptors.js",
    "local": "f=stripes.config.js; test -f $f.local && f=$f.local; echo Using config $f; stripes serve $f",
    "test": "echo 'No unit tests implemented'",
    "test-int": "stripes test nightmare stripes.config.js",
    "test-regression": "stripes test nightmare stripes.config.js --run WD/platform-core/checkout/users/inventory/requests/circulation/organization",
    "lint": "eslint test/ui-testing"
  },
  "dependencies": {
    "@folio/calendar": ">=0.0.0",
    "@folio/data-import": "folio-org/ui-data-import#use-stripes-framework",
    "@folio/eholdings": ">=1.1.0",
    "@folio/finance": ">=1.1.0",
    "@folio/orders": "folio-org/ui-orders#use-stripes-framework",
    "@folio/platform-core": ">=1.1.0",
    "@folio/plugin-find-vendor": "frontside-folio/ui-plugin-find-vendor#jc/stripes-1.0",
    "@folio/stripes": "^1.0.0",
    "@folio/vendors": ">=1.1.0",
    "react": "^16.3.0"
  },
  "resolutions": {
    "@folio/plugin-find-vendor": "frontside-folio/ui-plugin-find-vendor#jc/stripes-1.0"
  },
  "devDependencies": {
    "@folio/eslint-config-stripes": "^2.0.0",
    "@folio/stripes-cli": "^1.5.0",
    "eslint": "^4.19.1",
    "lodash": "^4.17.5"
  }
}
